{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2021",
        "outDir": "out",
        "rootDir": ".",
        "lib": [
            "ES2021"
        ],
        "strict": true,
        "sourceMap": true,
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}